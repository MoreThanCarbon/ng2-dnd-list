   },
   "keywords": [
      "angular",
      "drag and drop",
      "library",
      "directives"
   ],
   "author": "fran <fjsc.noah@gmail.com>",
   "license": "MIT",
   "peerDependencies": {
      "@angular/core": ">=4.0.0",
      "@angular/common": ">=4.0.0"
   },
   "main": "bundles/fjsc-ng2-dnd-list.umd.js",
   "module": "esm5/fjsc-ng2-dnd-list.js",
   "es2015": "esm2015/fjsc-ng2-dnd-list.js",
   "typings": "fjsc-ng2-dnd-list.d.ts",
   "metadata": "fjsc-ng2-dnd-list.metadata.json",
